.container{
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
}
.imgDiv {
  display: inline-block;
  position: relative;
  width: 150rpx;
  height: 150rpx;
  border-width: 4rpx;
  border-color:grey;
  border-style: solid;
  margin: 20rpx;
}
.imgDiv .delete {
  position: absolute;
  top: -10px;
  right: -10px;
  width: 20px;
  height: 20px;
}
.image-pre{
  height: 150rpx;
  width: 150rpx;
}